<odoo>
  <data>
      <!-- model.name inherit form view -->
      <record id="method_hr_lre.hr_afp_form" model="ir.ui.view">
        <field name="name">method.hr.lre.hr.afp.form</field>
        <field name="model">hr.afp</field>
        <field name="inherit_id" ref="l10n_cl_hr.hr_afp_form"/>
        <field name="arch" type="xml">
          <field name="rut" position="after">
            <field name="codigo_lre"/>
          </field>          
        </field>
      </record>

    <!-- model.name form view -->
    <record id="method_hr_lre.hr_ccaf_view_form" model="ir.ui.view">
      <field name="name">method.hr.lre.hr.ccaf.view.form</field>
      <field name="model">hr.ccaf</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <field name="codigo"/>
              <field name="name"/>
              <field name="codigo_lre"/>
              
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="method_hr_lre.hr_mutual_view_form" model="ir.ui.view">
      <field name="name">method.hr.lre.hr.mutual.view.form</field>
      <field name="model">hr.mutual</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <field name="codigo"/>
              <field name="name"/>
              <field name="codigo_lre"/>
              
            </group>
          </sheet>
        </form>
      </field>
    </record>

  </data>
</odoo>